    "@xmtp/message-kit": "latest",
    "@xmtp/mls-client": "^0.0.14",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "express": "^4.19.2",
    "openai": "^4.52.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
